{
  "name": "planner-api",
  "version": "0.1.0",
  "description": "",
  "main": "src/main.mjs",
  "dependencies": {
    "cron-parser": "^2.13.0",
    "flake-idgen": "^1.1.2",
    "lokijs": "^1.5.8",
    "mqtt": "^3.0.0",
    "node-schedule": "^1.3.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1"
  },
  "scripts": {
    "debug:watch": "onchange -v -i -k \"src/**/*.(mjs|js)\" -e \"src/test_client_mqtt.js\" -e \"**/node_modules/**\" -- npm run debug",
    "debug:watch:test_client_mqtt": "onchange -v -i -k \"src/test_client_mqtt.js\" \"src/mqtt-client/utilities.js\" -- npm run debug:test_client_mqtt",
    "debug": "run-s build start",
    "debug:test_client_mqtt": "run-s build:test_client_mqtt start:test_client_mqtt",
    "build": "babel --delete-dir-on-start src --ignore src/test_client_mqtt.js --out-dir dist --source-maps",
    "build:test_client_mqtt": "babel --delete-dir-on-start src/test_client_mqtt.js --out-dir dist_test_client_mqtt --source-maps && babel --delete-dir-on-start src/mqtt-client/utilities.js --out-dir dist_test_client_mqtt/mqtt-client --source-maps",
    "build:watch": "babel --delete-dir-on-start src --ignore src/test_client_mqtt.js --out-dir dist --source-maps --watch",
    "build:watch:test_client_mqtt": "babel --delete-dir-on-start src/test_client_mqtt.js --out-dir dist_test_client_mqtt --source-maps --watch && babel --delete-dir-on-start src/mqtt-client/utilities.js --out-dir dist_test_client_mqtt/mqtt-client --source-maps --watch",
    "start": "node dist/main.js",
    "start:test_client_mqtt": "node dist_test_client_mqtt/test_client_mqtt.js",
    "start:es6": "node src/main.js",
    "format": "prettier --write 'src/**/*.(mjs|js)'",
    "format:watch": "onchange \"src/**/*.(mjs|js)\" -- prettier --write {{changed}}",
    "lint": "eslint src --color",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lauri Saar",
  "license": "ISC"
}
